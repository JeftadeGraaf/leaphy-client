{
  "name": "easybloqs",
  "author": "Leaphy Robotics",
  "description": "Build Leaphy Arduino programs",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "clean:darwin": "rm -f easybloqs.dmg && rm -rf dist && rm -rf easybloqs-darwin-x64",
    "clean:linux": "rm -f easybloqs.dmg && rm -rf dist && rm -rf easybloqs-linux-x64",
    "start:electron": "ng build --base-href ./ && electron .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "aws s3 sync .\\dist s3://liquidlogic.eu --delete --profile ll-liquidlogic",
    "package:electron:darwin": "yarn install && ng build --prod --base-href ./ && electron-packager . --platform=darwin --ignore=/e2e --ignore=/src --ignore=/installers --ignore=/lib/win32 --ignore=/lib/linux --ignore=/easybloqs-darwin-x64 --overwrite --verbose --asar",
    "package:electron:linux": "yarn install && ng build --prod --base-href ./ && electron-packager . --platform=linux --ignore=/e2e --ignore=/src --ignore=/installers --ignore=/lib/darwin --ignore=/lib/win32 --ignore=/easybloqs-linux-x64 --overwrite --verbose --asar",
    "build-installer:electron:darwin": "electron-installer-dmg --debug --overwrite --out=./installers/darwin ./easybloqs-darwin-x64/easybloqs.app Easybloqs",
    "build-installer:electron:linux": "electron-installer-zip easybloqs-linux-x64/ installers/linux/easybloqs-linux-x64.zip",
    "installer:electron:darwin": "yarn run clean:darwin && yarn run package:electron:darwin && yarn run build-installer:electron:darwin",
    "installer:electron:linux": "yarn run clean:linux && yarn run package:electron:linux && yarn run build-installer:electron:linux",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular": "^1.8.1",
    "blockly": "github:leaphy-robotics/leaphy-blockly#86f113e90118943af9479579fb56d2ea30266140",
    "bootstrap": "^4.5.3",
    "electron-first-run": "^3.0.0",
    "electron-prompt": "^1.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "prismjs": "^1.22.0",
    "rxjs": "~6.4.0",
    "serialport": "^9.0.4",
    "tslib": "^1.14.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.64",
    "codelyzer": "^5.2.2",
    "electron": "^8.5.2",
    "electron-installer-zip": "^0.1.2",
    "electron-rebuild": "^2.3.4",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "easybloqs"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
