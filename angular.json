{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "leaphy-client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/leaphy-client",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/leaphy-blockly/blocklyduino/images/",
                "output": "./images/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"         
            ],
            "scripts": [
              "node_modules/leaphy-blockly/blockly/blockly_compressed.js",
              "node_modules/leaphy-blockly/blockly/blocks_compressed.js",
              "node_modules/leaphy-blockly/blockly/closure/goog/base.js",
              "node_modules/leaphy-blockly/blockly/msg/js/en.js",
              "node_modules/leaphy-blockly/blocklyduino/blocks/arduino_base/arduino_base.js",
              "node_modules/leaphy-blockly/blocklyduino/msg/blocks_en.js",
              "node_modules/leaphy-blockly/blocklyduino/themes/bw.js",
              "node_modules/leaphy-blockly/blocklyduino/themes/themes_blocks.js",
              "node_modules/leaphy-blockly/blocklyduino/js/arduino_boards.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino_generator.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/arduino_base.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/colour.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/lists.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/logic.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/loops.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/math.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/procedures.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/text.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/variables.js",
              "node_modules/leaphy-blockly/blocklyduino/generators/arduino/blockly/variables_dynamic.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "leaphy-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "leaphy-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "leaphy-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "leaphy-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "leaphy-client:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "leaphy-client"
}