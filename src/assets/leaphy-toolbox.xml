<xml xmlns="https://developers.google.com/blockly/xml" style="display: none">
    <category id="l_original" name="%{BKY_LEAPHY_ORIGINAL_CATEGORY}" categorystyle="leaphy_category">
      <block type="leaphy_original_set_led">
        <value name="LED_RED">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
        </value>
        <value name="LED_GREEN">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
        </value>
        <value name="LED_BLUE">
          <shadow type="math_number">
            <field name="NUM">0</field>
          </shadow>
        </value>
      </block>
      <block type="leaphy_original_set_motor">
        <value name="MOTOR_SPEED">
          <shadow type="math_number">
            <field name="NUM">100</field>
          </shadow>
        </value>
      </block>
      <block type="leaphy_original_get_distance"></block>
      <block type="leaphy_original_move_motors">
        <value name="MOTOR_SPEED">
          <shadow type="math_number">
            <field name="NUM">100</field>
          </shadow>
        </value>
      </block>
      <block type="leaphy_original_digital_read"></block>
      <block type="leaphy_original_analog_read"></block>
    </category>
	<category id="l_flitz" name="%{BKY_LEAPHY_FLITZ_CATEGORY}" categorystyle="leaphy_category">
		<block type="leaphy_flitz_read_stomach_sensor"></block>
		<block type="leaphy_flitz_read_hand_sensor"></block>
	</category>
	<category id="l_wifi" name="%{BKY_LEAPHY_WIFI_CATEGORY}" categorystyle="leaphy_category">
	</category>
	<category id="l_extra" name="%{BKY_LEAPHY_EXTRA_CATEGORY}" categorystyle="leaphy_category">
    <block type="leaphy_rgb_read_sensor"></block>
    <block type="leaphy_rgb_color_red"></block>
    <block type="leaphy_rgb_color_green"></block>
    <block type="leaphy_rgb_color_blue"></block>
    <block type="leaphy_rgb_raw_color_red"></block>
    <block type="leaphy_rgb_raw_color_green"></block>
    <block type="leaphy_rgb_raw_color_blue"></block>
	</category>
</xml>